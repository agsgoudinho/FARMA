<!DOCTYPE html>
<html>
<head>
  <%= favicon_link_tag %>
  <title><%= full_title(yield(:title)) %></title>
  <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
  <link href="//cdn.materialdesignicons.com/1.7.12/css/materialdesignicons.min.css" rel="stylesheet">

  <%= stylesheet_link_tag    'student/application', media: 'all', 'data-turbolinks-track' => "reload" %>
  <%= javascript_include_tag 'student/application', 'data-turbolinks-track' => "reload" %>
  <%= csrf_meta_tags %>
</head>
<body>

  <header>
    <%= render 'layouts/public/navbar'  %>

    <%= render 'shared/flash_messages'  %>
  </header>

  <main>
     <%= yield %>
  </main>
</body>
</html>
